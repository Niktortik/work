              "      <th>groups</th>\n",
              "      <th>ARPU</th>\n",
              "      <th>ARPAU</th>\n",
              "      <th>cr</th>\n",
              "      <th>cr_activ</th>\n",
              "      <th>cr_activ_math_byu</th>\n",
